package com.cb;

import java.util.ArrayList;

public class Level {
	public ArrayList<Tile> tiles;
	public ArrayList<GameObject> gameObjects;
	int width,height;
	
	public Level(int newWidth, int newHeight)
	{
		width = newWidth;
		height = newHeight;
		tiles = new ArrayList<Tile>();
		gameObjects = new ArrayList<GameObject>();
	}
}
