package com.cb;

public class Tile extends GameObject {
    int tileID; // ID of the tile, which is expressed in a tile map
    float layer; // Depth of the tile
    
    public Tile(int newTileID, float tileX, float tileY)
    {
    	tileID = newTileID;
    	layer = 0.0f;
    	x = tileX;
    	y = tileY;
    }
	
	public void draw()
	{
		
	}
}
